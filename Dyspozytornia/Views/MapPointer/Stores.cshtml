@{
    ViewData["Title"] = "Stores";
}

<body onLoad="load()">
<main>
    <section class="mainBody">
        <div class="row">
            <div class="title"><h1>Magazyny</h1></div>

            <div class="col-sm-12 mainText">
                <table>
                    <tr>
                        <th style="width: 10%"> Nazwa punktu </th>
                        <th style="display:none">Szerokosc geogr.</th>
                        <th style="display:none">Dlugosc geogr.</th>
                        <th> NIP </th>
                        <th> Miasto </th>
                        <th> Adres </th>
                        <th> Lokal </th>
                    </tr>
                    @Html.Raw(@ViewBag.table)
                </table>
                <div id="map" style="width: 500px; height: 500px; margin: 50px auto 0;"></div>
            </div>
        </div>
    </section>
</main>
</body>

<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCz3WIk3vjEwjCUZgtvq6O0630P8-0LWho&callback=initMap&sensor=true">
</script>